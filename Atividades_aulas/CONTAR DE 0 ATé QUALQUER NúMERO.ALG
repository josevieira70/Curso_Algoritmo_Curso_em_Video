algoritmo "Contar de 0 até qualquer número"
var
   valor, contador: inteiro
inicio
      Contador <- 0
      Escreva ("Quer contar ate quanto?")
      Leia (valor)
      Enquanto (contador <=valor) faca
               Escreval (contador)
               Contador <- contador + 1
      FimEnquanto
      Escreval ("Terminei de contar")
fimalgoritmo